{
  "kind": "collectionType",
  "collectionName": "roteiros",
  "info": {
    "singularName": "roteiro",
    "pluralName": "roteiros",
    "displayName": "Roteiro"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Titulo": {
      "type": "string",
      "required": true
    },
    "Slug": {
      "type": "uid",
      "targetField": "Titulo"
    },
    "Introducao": {
      "type": "text"
    },
    "Tags": {
      "type": "string"
    },
    "Destino": {
      "type": "string"
    },
    "Dificuldade": {
      "type": "enumeration",
      "enum": [
        "Tranquilo",
        "Moderado",
        "Aventureiro",
        "Desafiador"
      ]
    },
    "MelhorEpoca": {
      "type": "string"
    },
    "Destaque": {
      "type": "boolean"
    },
    "PlanoDiario": {
      "type": "component",
      "component": "plano-diario.plano-diario",
      "repeatable": true
    },
    "GaleriaDeFotos": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Preco": {
      "type": "decimal"
    },
    "Duracao": {
      "type": "string"
    },
    "Estilo_de_Viagem": {
      "type": "enumeration",
      "enum": [
        "Luxo",
        "Aventura",
        "Cultural",
        "Romantico",
        "Familia",
        "Festas Tradicionais",
        "Esportes"
      ]
    },
    "SEO": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    },
    "ChamadaParaAcao": {
      "type": "string"
    },
    "depoimento": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::depoimento.depoimento",
      "mappedBy": "roteiro_relacionado"
    }
  }
}
